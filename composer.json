{
    "name": "zhyu/tawa",
    "description": "admin with laravel, alpineJs and livewire",
    "license": "MIT",
    "keywords": [
        "laravel", "AlpineJs", "Livewire"
    ],
    "authors": [
        {
            "name": "karajanchang",
            "email": "karajanchang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "livewire/livewire": "^2.5",
        "illuminate/console": "^8.53",
        "illuminate/database": "^8.53",
        "illuminate/support": "^8.53",
        "spatie/laravel-permission": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Tawa\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tawa\\Tests\\Packages\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Tawa\\TawaServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
